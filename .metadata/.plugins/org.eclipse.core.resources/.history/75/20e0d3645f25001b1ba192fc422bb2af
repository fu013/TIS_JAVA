package Extends;

public class 피라미드형별 {

	public static void main(String[] args) {
		for (int i = 0; i < 10; i++) {
		    for (int j = 10-1; j > i; j--) { 
		        System.out.print(" ");
		        // i 값이 증가함에 따라 공백수가 감소하는 포문
		    }
		    for (int k = 0; k < 2*i+1; k++) { 
		        System.out.print("*"); 
		    } 
		        System.out.println(); 
		}
	}

}
