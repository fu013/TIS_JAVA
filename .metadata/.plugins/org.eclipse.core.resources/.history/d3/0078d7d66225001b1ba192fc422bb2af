package Extends;

public class 구구단 {
	public static void main(String[] args) {
		for (int i = 2; i &lt; 10; i++) {
            System.out.println(i + "단");
            for (int j = 1; j &lt; 10; j++) { // 이중 for 문
                System.out.println(i + "*" + j + "=" + (i*j)); 
            }
        }
	}
}
